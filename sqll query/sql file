CREATE TABLE `yk_ex`.`master` (
  `client_id` INT NOT NULL,
  `client_name` VARCHAR(45) NOT NULL,
  `contact_person_name` VARCHAR(45) NOT NULL,
  `contact_person_designation` VARCHAR(45) NOT NULL,
  `contact_person_number` INT NOT NULL,
  `payment_terms` VARCHAR(45) NOT NULL,
  `cv_ownership` VARCHAR(45) NOT NULL,
  `replacement_validity` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`client_id`),
  UNIQUE INDEX `client_id_UNIQUE` (`client_id` ASC) VISIBLE);



CREATE TABLE open_positions (
  position_id int NOT NULL,
  client_id int NOT NULL,
    position_title int NOT NULL,
    salary int NOT NULL,
    location int NOT NULL,
    experience int NOT NULL,
    position_status varchar(10) NOT NULL,
    PRIMARY KEY (position_id),
    FOREIGN KEY (client_id) REFERENCES master(client_id)
);




CREATE TABLE agreemeent_status (
  agreement_id int NOT NULL,
  client_id int NOT NULL,
    P_AND_P_sent varchar(50) NOT NULL,
    P_AND_P_revised varchar(50) NOT NULL,
    agreement_sent varchar(50) NOT NULL,
    agreement_signed varchar(50) NOT NULL,
    agreement_revised varchar(10) NOT NULL,
    PRIMARY KEY (agreement_id),
    FOREIGN KEY (client_id) REFERENCES master(client_id)
);



create table revenue_percentge (
r_percent int NOT NULL,
client_id int NOT NULL,
PRIMARY KEY (r_percent),
FOREIGN KEY (client_id) REFERENCES master(client_id)

);



